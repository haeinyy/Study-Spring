package com.ssafy.hw2.model;

import org.apache.commons.lang3.builder.ToStringBuilder;

public class CateVO {
	
	private int sep;
	private String item_code;
	private String item_name;
	private int item_price;
	private String item_corp;
	private char item_stat;
	private String dt_reg;
	private int cate_seq;
	
	public CateVO() {};
	
	public CateVO(int sep, String item_code, String item_name, int item_price, String item_corp, char item_stat,
			String dt_reg, int cate_seq) {
		super();
		this.sep = sep;
		this.item_code = item_code;
		this.item_name = item_name;
		this.item_price = item_price;
		this.item_corp = item_corp;
		this.item_stat = item_stat;
		this.dt_reg = dt_reg;
		this.cate_seq = cate_seq;
	}
	
	
	
}