<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<!-- bean은 spring이 관리하는 객체  -->
<!-- 빈생성하기 -->
	<bean id="kor" class="com.ssafy.hello.di4.HelloMessageKor"></bean> <!-- 기본은 singleton : xml 읽어들이는 시점에서 생성-->
	<!-- <bean id="kor" class="com.ssafy.hello.di4.HelloMessageKor" scope="prototype"></bean>  --><!-- 새로운 인스턴스 반환하고 싶으면 prototype 지정 --><!-- 호출하는 시점에서 생성 -->
	<bean id="eng" class="com.ssafy.hello.di4.HelloMessageEng"></bean>
</beans>
